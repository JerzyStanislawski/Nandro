    <themes:BundledTheme BaseTheme="Light" PrimaryColor="LightBlue" SecondaryColor="Blue" />
  </Application.Resources>
    <Application.Styles>
      <StyleInclude Source="avares://Material.Avalonia/Material.Avalonia.Templates.xaml" />
